<?xml version="1.0" encoding="iso-8859-2" ?>
<project name="SimpleAntProject">
	
	<property name="build.dir" location=".."/>
	<property name="src.dir" location="src/"></property>
	<target name="prepare">
		<mkdir dir="${build.dir}"/>
	</target>
	<target name="compile">
		<javac>
			<src path="${src.dir}"/>
		</javac>
	</target>
</project>
